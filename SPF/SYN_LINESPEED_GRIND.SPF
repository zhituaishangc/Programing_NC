PROC SYN_LINESPEED_GRIND SBLOF DISPLOF
;砂轮速度

DEF REAL WHEEL_SYN_RPM;砂轮转速

DRESSER[91]=DRESSER[90]-(DRESSER[70]-DRESSER[88])*2;当前砂轮直径计算

WHEEL_SYN_RPM= PROCESS[23]*60000/($PI*DRESSER[91]);计算外圆磨过程中砂轮转速

R295=WHEEL_SYN_RPM
G4 F0.2
M??;开外圆磨砂轮

IF TECH_TIME[5]+TECH_TIME[6]+TECH_TIME[7]+TECH_TIME[8]+TECH_TIME[9]<=1
    MSG("等待砂轮转速到位")
    G4 F5
ELSE
    G4 F0.5
ENDIF

RET

