PROC SYN_LINESPEED_DRESS SBLOF DISPLOF
;砂轮速度

DEF REAL WHEEL_SYN_RPM2;砂轮转速

DRESSER[91]=DRESSER[90]-(DRESSER[70]-DRESSER[88])*2;当前砂轮直径计算

WHEEL_SYN_RPM2= WHEEL_LINESPEED_DRESS[14]*60000/($PI*DRESSER[91]);计算外圆磨修砂轮时砂轮转速

R295=WHEEL_SYN_RPM2
G4 F0.2
M??;开外圆磨砂轮

IF DRESSER[73]+DRESSER[74]<=1
    MSG("等待砂轮转速到位")
    G4 F5
ELSE
    G4 F0.5
ENDIF

RET

