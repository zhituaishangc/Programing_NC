PROC S_DRESS_START DISPLOF
;报警及参数初始化

DRESSER[18]=0;当前次数清零
DRESSER[19]=0
DRESSER[20]=0
DRESSER[6]=0

IF DRESSER[24]<DRESSER[35]
	MSG("砂轮1过小,请更换砂轮!")
	G4 F3
	DRESSER[6]=1;报警标记
	GOTOF ENDING_A
ENDIF
IF DRESSER[38]<DRESSER[44];当前砂轮2直径<最小砂轮2直径
	MSG("砂轮2过小,请更换砂轮!")
	G4 F3
	DRESSER[6]=1;报警标记
	GOTOF ENDING_A
ENDIF

ENDING_A:
RET

