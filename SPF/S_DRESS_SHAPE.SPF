PROC S_DRESS_SHAPE DISPLON
;齿形选择

IF DRESSER[18]+DRESSER[19]+DRESSER[20]==0;第一遍跳出
    ;RET不需要跳出
ENDIF

CASE DRESSER[4] OF 0 GOTOF SHAPE_0 1 GOTOF SHAPE_1 DEFAULT GOTOF SHAPE_2

SHAPE_0:
S_TRIANGLE;三角齿
DRESSER[5]=1;旧砂轮
RET

SHAPE_1:
S_TRAPEZOIDAL;梯形
DRESSER[5]=1
RET

SHAPE_2:
S_DOUBLE_ARC;双圆弧
DRESSER[5]=1
RET

