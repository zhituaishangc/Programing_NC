PROC SN_RIGHT_FACE_GRIND DISPLOF
;磨削中心右端面磨削

MSG("X轴/U轴回退至安全位置")
G90 G1 X=INI[29] U=INI[30] F=2000

STOPRE

G90 G1 Z=TOOL_SET[22]+0.1 F2000  ;Z轴开至右端面磨削起点外
G90 G1 U=TOOL_SET[23] F2000  ;U轴开至外圆磨削起点

G4 F2
MSG("C轴启动，右端面磨削开始")
M3 S10
G90 G1 U=TOOL_SET[23]-PROCESS[27] F=PROCESS[24] 
G90 G1 Z=TOOL_SET[22]-PROCESS[26] F=PROCESS[24] 


RET

