PROC SN_LEFT_FACE_GRIND DISPLOF
;磨削中心左端面磨削

G90 G1 X=INI[29] U=INI[30] F=2000          ;X/U轴退回安全位置
G90 G1 Z=TOOL_SET[24]+0.1 F1000            ;Z轴开至磨削位置
G90 G1 U=TOOL_SET[23]+1 F2000              ;U轴开至磨削位置

MSG("C轴启动,外圆磨削开始")

M3 S=PROCESS[29]                           ;C轴启动

M26	                                       ;控制箱空调启动
M20                                        ;吸雾器启动
M38                                        ;磨削冷却开

TECHNOLOGY[98]=0                           ;循环计数清空

WHILE TECHNOLOGY[98]<TECHNOLOGY[99]

MSG("左端面磨削，当前为第"<< TECHNOLOGY[98]+1<<"次磨削，还剩"<<TECHNOLOGY[99]-1-TECHNOLOGY[98]<<"次，进给量为"<<PROCESS[34]<<"mm")

G94 G90 G01 Z=TOOL_SET[24]-PROCESS[34] U=TOOL_SET[23]+0.1 F1         ;Z/U插补法进刀 磨削左端面

G4 F3

G94 G90 G01 Z=TOOL_SET[24]-PROCESS[34]+0.1 U=TOOL_SET[23]+1 F1000    ;Z/U插补退刀 

STOPRE

TECHNOLOGY[98]=TECHNOLOGY[98]+1             ;磨削循环计数
TOOL_SET[24]=TOOL_SET[24]-PROCESS[34]       ;磨削量累计

ENDWHILE

STOPRE

M39                                         ;磨削冷却关
M21	                                        ;吸雾器停止
M27	                                        ;控制箱空调停止

G90 G1 Z=TOOL_SET[24]+0.5 F3000             ;退刀
G90 G1 U=INI[30] F=3000                     ;退刀


RET

