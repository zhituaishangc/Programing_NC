PROC S_ROTATE_ANGLE DISPLOF
;;**********程序功能**********
;;螺旋升角
;;***************************

IF INI[16]==1
	IF TRUNC($AA_IM[A]*1000)<>TRUNC(INI[20]*1000)
		M33;液压开
		G4 F1
		M35;松开
		WHILE($A_DBB[3]==0);液压开关未到位
			G4 F0.2
		ENDWHILE
		G4 F1
		MSG("螺旋升角到位中")
		G90 G01 A=INI[20] F=INI[60]
		STOPRE
		G4 F0.5
		M36;夹紧
		G4 F1
		M34;液压关
	ENDIF
ENDIF

RET

