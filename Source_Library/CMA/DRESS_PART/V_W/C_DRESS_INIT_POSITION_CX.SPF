PROC C_DRESS_INIT_POSITION_CX DISPLOF
;***********程序功能**********
;*修整初始移动位置
;****************************

MSG("修整轮运行至起始位置")
G90 G1 V=$AA_SOFTENDP[V]-1 F=INI[61]

;ifIsVWBegin
MSG("水平初始位")
G90 G01 W=DRESSER[21] F=INI[63]
;ifIsVWEnd

MSG("修整轮运行至修整位置")
G90 G1 V=WHEEL[10]+1 F=INI[62]
G90 G1 V=WHEEL[10]+0.1 F=200

RET

