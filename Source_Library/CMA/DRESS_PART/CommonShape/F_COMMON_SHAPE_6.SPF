PROC F_COMMON_SHAPE_6 DISPLOF
;***********程序功能**********
;*双圆弧
;****************************

SHAPE_MODEL[85]=0;过度1斜线长度延长
SHAPE_MODEL[86]=SHAPE_MODEL[84];过度2斜线角度
IF SHAPE_MODEL[87]<DRESSER[32]
    SHAPE_MODEL[87]=DRESSER[32]
ENDIF

COMMON_ANG[2]=90-SHAPE_MODEL[88]
COMMON_ANG[3]=90-SHAPE_MODEL[84]
COMMON_ANG[4]=90-SHAPE_MODEL[86]
COMMON_ANG[5]=90
COMMON_ANG[7]=90-SHAPE_MODEL[88]
COMMON_ANG[8]=90-SHAPE_MODEL[84]
COMMON_ANG[9]=90-SHAPE_MODEL[86]
COMMON_ANG[10]=90

COMMON_RND[1]=SHAPE_MODEL[81]
COMMON_RND[2]=DRESSER[32]+0.05
COMMON_RND[3]=0
COMMON_RND[4]=0
COMMON_RND[5]=0
COMMON_RND[6]=SHAPE_MODEL[80]
COMMON_RND[7]=DRESSER[32]+0.05
COMMON_RND[8]=0
COMMON_RND[9]=0
COMMON_RND[10]=0

COMMON_LENGTH[4]=SHAPE_MODEL[87]
COMMON_LENGTH[9]=SHAPE_MODEL[87]

E_COMMON_DARC_ANG(COMMON_RND[1],SHAPE_MODEL[83],COMMON_ANG[1])
E_COMMON_DARC_ANG(COMMON_RND[6],SHAPE_MODEL[82],COMMON_ANG[6])

E_COMMON_SHAPE_RND;检查圆角半径是否正确

E_COMMON_LINE_TO_ARC(180,COMMON_ANG[1],COMMON_ANG[2],0,COMMON_RND[1],COMMON_LENGTH[1])
E_COMMON_LINE_TO_ARC(180,COMMON_ANG[6],COMMON_ANG[7],0,COMMON_RND[6],COMMON_LENGTH[6])

E_COMMON_LINE_TO_ARC(COMMON_ANG[1],COMMON_ANG[2],COMMON_ANG[3],COMMON_RND[1],COMMON_RND[2],COMMON_LENGTH[2])
E_COMMON_LINE_TO_ARC(COMMON_ANG[6],COMMON_ANG[7],COMMON_ANG[8],COMMON_RND[6],COMMON_RND[7],COMMON_LENGTH[7])

E_COMMON_LINE_TO_ARC(COMMON_ANG[2],COMMON_ANG[3],COMMON_ANG[4],COMMON_RND[2],COMMON_RND[3],COMMON_LENGTH[3])
COMMON_LENGTH[3]=COMMON_LENGTH[3]+SHAPE_MODEL[85]

E_COMMON_LINE_TO_ARC(COMMON_ANG[7],COMMON_ANG[8],COMMON_ANG[9],COMMON_RND[7],COMMON_RND[8],COMMON_LENGTH[8])
COMMON_LENGTH[8]=COMMON_LENGTH[8]+SHAPE_MODEL[85]

RET

