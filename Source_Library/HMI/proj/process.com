				VAR11.WR=2
				VAR12.WR=2
				VAR13.WR=2
				VAR19.WR=2 ;ifIsRoughToAmount
				VAR20.WR=2
				VAR21.WR=2
				VAR22.WR=2
			ENDIF
		ENDIF	
